{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport productos from './assets/js/productosTemporada.json';\nimport $ from 'jquery';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      currentRoute: 'index',\n      productos: productos // Importamos los productos del JSON\n    };\n  },\n  methods: {\n    changeRoute(route) {\n      this.currentRoute = route;\n\n      // Cambiar el título según la ruta seleccionada\n      if (route === 'index') {\n        document.title = 'Inicio';\n      } else if (route === 'catalogo') {\n        document.title = 'Catálogo de Productos';\n      } else if (route === 'carrito') {\n        document.title = 'Carrito de Compras';\n      }\n\n      // Si la ruta es 'index', reiniciamos el slider\n      if (route === 'index') {\n        this.$nextTick(() => {\n          this.initializeSlider();\n        });\n      } else {\n        this.destroySlider();\n      }\n    },\n    // Método para obtener la URL de la imagen\n    getImageUrl(imagePath) {\n      return require(`@/assets/img/${imagePath}`);\n    },\n    // Método para inicializar el slick slider\n    initializeSlider() {\n      $(this.$el).find('.slider').slick({\n        speed: 10000,\n        autoplay: false,\n        autoplaySpeed: 0,\n        cssEase: 'linear',\n        slidesToShow: 5,\n        slidesToScroll: 1,\n        infinite: true,\n        swipeToSlide: true,\n        centerMode: true,\n        focusOnSelect: true,\n        dots: false,\n        arrows: false\n      });\n    },\n    // Método para destruir el slick slider\n    destroySlider() {\n      const slider = $(this.$el).find('.slider');\n      if (slider.hasClass('slick-initialized')) {\n        slider.slick('unslick');\n      }\n    }\n  },\n  mounted() {\n    // Inicializamos el slider al cargar la página\n    document.title = 'Inicio';\n    this.initializeSlider();\n  },\n  beforeUnmount() {\n    // Aseguramos destruir el slider cuando el componente se destruye\n    this.destroySlider();\n  }\n};","map":{"version":3,"names":["productos","$","name","data","currentRoute","methods","changeRoute","route","document","title","$nextTick","initializeSlider","destroySlider","getImageUrl","imagePath","require","$el","find","slick","speed","autoplay","autoplaySpeed","cssEase","slidesToShow","slidesToScroll","infinite","swipeToSlide","centerMode","focusOnSelect","dots","arrows","slider","hasClass","mounted","beforeUnmount"],"sources":["C:\\xampp\\htdocs\\prueba-feat\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"__body\">\n\n    <div class=\"__header\">\n      <div class=\"container-in\">\n        <nav>\n          <button @click=\"changeRoute('index')\">Inicio</button>\n          <button @click=\"changeRoute('catalogo')\">Catálogo</button>\n          <button @click=\"changeRoute('carrito')\">Carrito</button>\n        </nav>\n      </div>\n    </div>\n    \n\n    <div v-if=\"currentRoute === 'index'\" class=\"__home\">\n      <div class=\"__cover\">\n        <div class=\"__backCover\">\n          <img :src=\"require('@/assets/img/_backCover.png')\"/>\n        </div>\n        <div class=\"__coverImg\">\n          <img :src=\"require('@/assets/img/__coverDesktop.png')\" class=\"desktop\"/>\n          <img :src=\"require('@/assets/img/__coverResponsive.png')\" class=\"responsive\"/>\n        </div>\n      </div>\n      <div class=\"__productosTemp\">\n        <div class=\"container-in\">\n          <div class=\"__productosTempTitle\">\n            <img :src=\"require('@/assets/img/__titleProductosDeTemporada.png')\"/>\n          </div>\n        </div>\n        <div class=\"__sliderContent\">  \n          <div class=\"slider\">\n            <div v-for=\"producto in productos\" :key=\"producto.nombre\" class=\"producto\">\n              <img :src=\"getImageUrl(producto.imagen)\" alt=\"Producto\" />\n              <h3>{{ producto.nombre }}</h3>\n              <p>GTQ.{{ producto.precio }}</p>\n              <a href=\"#\">Agregar a carrito</a>\n            </div>\n          </div>\n          <div class=\"__ctaProductos\">\n            <button @click=\"changeRoute('catalogo')\">Catálogo</button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n\n\n    <div v-if=\"currentRoute === 'catalogo'\">\n      <h1>Catálogo de productos</h1>\n      <p>Aquí puedes ver nuestros productos disponibles.</p>\n    </div>\n\n\n\n    <div v-if=\"currentRoute === 'carrito'\">\n      <h1>Tu carrito</h1>\n      <p>Estos son los productos en tu carrito.</p>\n    </div>\n\n\n  </div>\n</template>\n<script>\n  import productos from './assets/js/productosTemporada.json';\n  import $ from 'jquery';\n\n  export default {\n    name: 'App',\n    data() {\n      return {\n        currentRoute: 'index',\n        productos: productos, // Importamos los productos del JSON\n      };\n    },\n    methods: {\n      changeRoute(route) {\n        this.currentRoute = route;\n\n        // Cambiar el título según la ruta seleccionada\n        if (route === 'index') {\n          document.title = 'Inicio';\n        } else if (route === 'catalogo') {\n          document.title = 'Catálogo de Productos';\n        } else if (route === 'carrito') {\n          document.title = 'Carrito de Compras';\n        }\n\n        // Si la ruta es 'index', reiniciamos el slider\n        if (route === 'index') {\n          this.$nextTick(() => {\n            this.initializeSlider();\n          });\n        } else {\n          this.destroySlider();\n        }\n      },\n\n      // Método para obtener la URL de la imagen\n      getImageUrl(imagePath) {\n        return require(`@/assets/img/${imagePath}`);\n      },\n\n      // Método para inicializar el slick slider\n      initializeSlider() {\n        $(this.$el).find('.slider').slick({\n          speed: 10000,\n          autoplay: false,\n          autoplaySpeed: 0,\n          cssEase: 'linear',\n          slidesToShow: 5,\n          slidesToScroll: 1,\n          infinite: true,\n          swipeToSlide: true,\n          centerMode: true,\n          focusOnSelect: true,\n          dots: false,\n          arrows: false,\n        });\n      },\n\n      // Método para destruir el slick slider\n      destroySlider() {\n        const slider = $(this.$el).find('.slider');\n        if (slider.hasClass('slick-initialized')) {\n          slider.slick('unslick');\n        }\n      },\n    },\n\n    mounted() {\n      // Inicializamos el slider al cargar la página\n      document.title = 'Inicio';\n      this.initializeSlider();\n    },\n\n    beforeUnmount() {\n      // Aseguramos destruir el slider cuando el componente se destruye\n      this.destroySlider();\n    },\n  };\n</script>\n\n"],"mappings":";;AAgEE,OAAOA,SAAQ,MAAO,qCAAqC;AAC3D,OAAOC,CAAA,MAAO,QAAQ;AAEtB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,OAAO;MACrBJ,SAAS,EAAEA,SAAS,CAAE;IACxB,CAAC;EACH,CAAC;EACDK,OAAO,EAAE;IACPC,WAAWA,CAACC,KAAK,EAAE;MACjB,IAAI,CAACH,YAAW,GAAIG,KAAK;;MAEzB;MACA,IAAIA,KAAI,KAAM,OAAO,EAAE;QACrBC,QAAQ,CAACC,KAAI,GAAI,QAAQ;MAC3B,OAAO,IAAIF,KAAI,KAAM,UAAU,EAAE;QAC/BC,QAAQ,CAACC,KAAI,GAAI,uBAAuB;MAC1C,OAAO,IAAIF,KAAI,KAAM,SAAS,EAAE;QAC9BC,QAAQ,CAACC,KAAI,GAAI,oBAAoB;MACvC;;MAEA;MACA,IAAIF,KAAI,KAAM,OAAO,EAAE;QACrB,IAAI,CAACG,SAAS,CAAC,MAAM;UACnB,IAAI,CAACC,gBAAgB,CAAC,CAAC;QACzB,CAAC,CAAC;MACJ,OAAO;QACL,IAAI,CAACC,aAAa,CAAC,CAAC;MACtB;IACF,CAAC;IAED;IACAC,WAAWA,CAACC,SAAS,EAAE;MACrB,OAAOC,OAAO,CAAC,gBAAgBD,SAAS,EAAE,CAAC;IAC7C,CAAC;IAED;IACAH,gBAAgBA,CAAA,EAAG;MACjBV,CAAC,CAAC,IAAI,CAACe,GAAG,CAAC,CAACC,IAAI,CAAC,SAAS,CAAC,CAACC,KAAK,CAAC;QAChCC,KAAK,EAAE,KAAK;QACZC,QAAQ,EAAE,KAAK;QACfC,aAAa,EAAE,CAAC;QAChBC,OAAO,EAAE,QAAQ;QACjBC,YAAY,EAAE,CAAC;QACfC,cAAc,EAAE,CAAC;QACjBC,QAAQ,EAAE,IAAI;QACdC,YAAY,EAAE,IAAI;QAClBC,UAAU,EAAE,IAAI;QAChBC,aAAa,EAAE,IAAI;QACnBC,IAAI,EAAE,KAAK;QACXC,MAAM,EAAE;MACV,CAAC,CAAC;IACJ,CAAC;IAED;IACAlB,aAAaA,CAAA,EAAG;MACd,MAAMmB,MAAK,GAAI9B,CAAC,CAAC,IAAI,CAACe,GAAG,CAAC,CAA