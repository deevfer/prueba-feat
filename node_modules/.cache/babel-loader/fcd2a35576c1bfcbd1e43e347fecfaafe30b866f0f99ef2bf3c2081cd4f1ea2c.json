{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport productos from './assets/js/productosTemporada.json';\nimport $ from 'jquery';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      currentRoute: 'index',\n      productos: productos // Importamos los productos del JSON\n    };\n  },\n  methods: {\n    // Método para obtener la URL de la imagen\n    getImageUrl(imagePath) {\n      return require(`@/assets/img/${imagePath}`);\n    }\n  },\n  mounted() {\n    // Metodo para inicializar slick slider\n    $(this.$el).find('.slider').slick({\n      dots: true,\n      // Muestra los puntos de navegación\n      infinite: true,\n      // Carrusel infinito\n      speed: 500,\n      // Velocidad de la transición\n      fade: true,\n      // Efecto de desvanecimiento\n      cssEase: 'linear'\n    });\n  }\n};","map":{"version":3,"names":["productos","$","name","data","currentRoute","methods","getImageUrl","imagePath","require","mounted","$el","find","slick","dots","infinite","speed","fade","cssEase"],"sources":["C:\\xampp\\htdocs\\prueba-feat\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Menú de navegación -->\n    <nav>\n      <button @click=\"changeRoute('index')\">Inicio</button>\n      <button @click=\"changeRoute('catalogo')\">Catálogo</button>\n      <button @click=\"changeRoute('carrito')\">Carrito</button>\n    </nav>\n\n    <!-- Contenido que cambia según la ruta -->\n    <div v-if=\"currentRoute === 'index'\">\n      <h1>Bienvenido a la página principal</h1>\n      <p>Este es el índice de nuestra aplicación.</p>\n      <div class=\"slider\">\n        <div v-for=\"producto in productos\" :key=\"producto.nombre\" class=\"producto\">\n          <img :src=\"getImageUrl(producto.imagen)\" alt=\"Producto\" />\n          <h3>{{ producto.nombre }}</h3>\n          <p>Precio: GTQ.{{ producto.precio }}</p>\n        </div>\n      </div>\n    </div>\n\n    <div v-if=\"currentRoute === 'catalogo'\">\n      <h1>Catálogo de productos</h1>\n      <p>Aquí puedes ver nuestros productos disponibles.</p>\n    </div>\n\n    <div v-if=\"currentRoute === 'carrito'\">\n      <h1>Tu carrito</h1>\n      <p>Estos son los productos en tu carrito.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport productos from './assets/js/productosTemporada.json';\nimport $ from 'jquery';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      currentRoute: 'index',\n      productos: productos,   // Importamos los productos del JSON\n    };\n  },\n  methods: {\n    // Método para obtener la URL de la imagen\n    getImageUrl(imagePath) {\n      return require(`@/assets/img/${imagePath}`);\n    }\n  },\n  \n  mounted() {\n    // Metodo para inicializar slick slider\n    $(this.$el).find('.slider').slick({\n      dots: true,  // Muestra los puntos de navegación\n      infinite: true,  // Carrusel infinito\n      speed: 500,  // Velocidad de la transición\n      fade: true,  // Efecto de desvanecimiento\n      cssEase: 'linear'\n    });\n  }\n};\n</script>\n<style scoped>\nnav {\n  margin: 20px;\n}\n\nbutton {\n  margin-right: 10px;\n}\n</style>\n"],"mappings":";;AAmCA,OAAOA,SAAQ,MAAO,qCAAqC;AAC3D,OAAOC,CAAA,MAAO,QAAQ;AACtB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,OAAO;MACrBJ,SAAS,EAAEA,SAAS,CAAI;IAC1B,CAAC;EACH,CAAC;EACDK,OAAO,EAAE;IACP;IACAC,WAAWA,CAACC,SAAS,EAAE;MACrB,OAAOC,OAAO,CAAC,gBAAgBD,SAAS,EAAE,CAAC;IAC7C;EACF,CAAC;EAEDE,OAAOA,CAAA,EAAG;IACR;IACAR,CAAC,CAAC,IAAI,CAACS,GAAG,CAAC,CAACC,IAAI,CAAC,SAAS,CAAC,CAACC,KAAK,CAAC;MAChCC,IAAI,EAAE,IAAI;MAAG;MACbC,QAAQ,EAAE,IAAI;MAAG;MACjBC,KAAK,EAAE,GAAG;MAAG;MACbC,IAAI,EAAE,IAAI;MAAG;MACbC,OAAO,EAAE;IACX,CAAC,CAAC;EACJ;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}